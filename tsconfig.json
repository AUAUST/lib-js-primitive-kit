{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist/types",
    "declarationMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "~/*": ["./src/*"],
      "@auaust/primitive-kit": ["./src/index.ts"],
      "@auaust/primitive-kit/arrays": ["./src/arrays/methods/index.ts"],
      "@auaust/primitive-kit/booleans": ["./src/booleans/methods/index.ts"],
      "@auaust/primitive-kit/functions": ["./src/functions/methods/index.ts"],
      "@auaust/primitive-kit/numbers": ["./src/numbers/methods/index.ts"],
      "@auaust/primitive-kit/objects": ["./src/objects/methods/index.ts"],
      "@auaust/primitive-kit/primitives": ["./src/primitives/methods/index.ts"],
      "@auaust/primitive-kit/strings": ["./src/strings/methods/index.ts"]
    }
  },
  "include": ["src", "tests"],
  "exclude": ["node_modules", "dist"]
}

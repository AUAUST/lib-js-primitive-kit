{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "~/*": ["./src/*"]
    },
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true
  }
}
